<div class="col-md-12">
  <div class="job">
    <%= link_to("Edit",edit_job_path(@job),class:"btn btn-primary pull-right") %>
  </div>
  <h2> <%= @job.title  %></h2>
  <p> <td> 职位描述： </td> <td><%= @job.description %> </td></p>
  <p> <td> 薪资上限： </td> <%= @job.wage_lower_bound %></p>
  <p> <td> 职位下限： </td> <%= @job.wage_upper_bound %></p>
  <p> <td> 联系电话： </td> <%= @job.contact_email %>
   <div>
  <%= link_to("Back",jobs_path) %>
  </div>
</div>
