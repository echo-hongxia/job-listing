<h1> 编辑职位信息 </h1>

<%= simple_form_for [:admin,@job] do |f|%>

  <%= f.input :title ,:label =>"职位名" %>
  <%= f.input :description ,:label =>"职位描述" %>
  <%= f.input :wage_lower_bound,:label =>"薪资下限"  %>
  <%= f.input :wage_upper_bound,:label =>"薪资上限"%>
  <%= f.input :company,:label=> "公司名" %>
  <%= f.input :place,:label => "工作地点"%>
  <%= f.input :contact_email ,:label =>"联系方式"%>
  <%= f.input :is_hidden %>
  <%= f.submit "提交"%>
<% end %>
